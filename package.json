{
  "name": "dionysus-example",
  "description": "Hugo Dionysus Theme Example",
  "private": true,
  "author": "Sonatype, Inc.",
  "version": "0.0.1",
  "repository": "",
  "scripts": {
    "theme-build": "brunch build themes/project-site",
    "theme-watch": "brunch watch themes/project-site",
    "theme-wait": "wait-on themes/project-site/static/theme.js themes/project-site/static/theme.css",
    "hugo-server": "yarn theme-wait && hugo server --watch --buildDrafts --disableFastRender",
    "start": "concurrently -k -n brunch,hugo 'yarn theme-watch' 'yarn hugo-server'",
    "build": "brunch build --production themes/project-site && hugo"
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "wait-on": "^2.1.0"
  },
  "devDependencies": {}
}
