<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd">

  <skin>
    <groupId>org.sonatype.dionysus</groupId>
    <artifactId>dionysus-maven-skin</artifactId>
    <version>1-SNAPSHOT</version>
  </skin>

  <publishDate position="right"/>
  <version position="right"/>

  <custom>
    <fluidoSkin>
      <navBarStyle>navbar-inverse</navBarStyle>
      <topBarEnabled>true</topBarEnabled>
      <topBarIcon>
        <name>project</name>
        <src>https://jdillon.github.io/dionysus-example/images/sonatype-dark-32x32.png</src>
        <href>https://jdillon.github.io/dionysus-example/</href>
      </topBarIcon>
      <sideBarEnabled>false</sideBarEnabled>
    </fluidoSkin>
  </custom>

  <body>
    <breadcrumbs>
      <item name="Home" href="../index.html"/>
      <item name="Maven" href="./index.html"/>
    </breadcrumbs>

    <menu name="Modules" ref="modules"/>

    <menu name="Reports" ref="reports"/>

    <footer>
      ${dionysus.copyright}
    </footer>
  </body>
</project>