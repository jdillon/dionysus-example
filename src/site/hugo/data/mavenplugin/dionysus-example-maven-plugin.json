{"plugin": {
  "goalPrefix": "dionysus-example",
  "inheritedByDefault": true,
  "isolatedRealm": false,
  "groupId": "org.sonatype.dionysus",
  "name": "org.sonatype.dionysus:dionysus-example-maven-plugin",
  "mojos": {"mojo": [
    {
      "inheritedByDefault": true,
      "goal": "help",
      "configuration": {
        "goal": {
          "implementation": "java.lang.String",
          "content": "${goal}"
        },
        "detail": {
          "default-value": false,
          "implementation": "boolean",
          "content": "${detail}"
        },
        "indentSize": {
          "default-value": 2,
          "implementation": "int",
          "content": "${indentSize}"
        },
        "lineLength": {
          "default-value": 80,
          "implementation": "int",
          "content": "${lineLength}"
        }
      },
      "requiresProject": false,
      "aggregator": false,
      "implementation": "org.sonatype.dionysus.example.maven.HelpMojo",
      "description": "Display help information on dionysus-example-maven-plugin.<br>\nCall <code>mvn dionysus-example:help -Ddetail=true -Dgoal=&lt;goal-name&gt;<\/code> to display parameter details.",
      "threadSafe": true,
      "requiresReports": false,
      "language": "java",
      "requiresDirectInvocation": false,
      "requiresOnline": false,
      "instantiationStrategy": "per-lookup",
      "executionStrategy": "once-per-session",
      "parameters": {"parameter": [
        {
          "editable": true,
          "name": "detail",
          "description": "If <code>true<\/code>, display all settable properties for each goal.",
          "type": "boolean",
          "required": false
        },
        {
          "editable": true,
          "name": "goal",
          "description": "The name of the goal for which to show help. If unspecified, all goals will be displayed.",
          "type": "java.lang.String",
          "required": false
        },
        {
          "editable": true,
          "name": "indentSize",
          "description": "The number of spaces per indentation level, should be positive.",
          "type": "int",
          "required": false
        },
        {
          "editable": true,
          "name": "lineLength",
          "description": "The maximum length of a display line, should be positive.",
          "type": "int",
          "required": false
        }
      ]}
    },
    {
      "inheritedByDefault": true,
      "phase": "generate-resources",
      "goal": "helper",
      "requiresDependencyResolution": "compile",
      "configuration": {
        "settings": {
          "default-value": "${settings}",
          "implementation": "org.apache.maven.settings.Settings"
        },
        "session": {
          "default-value": "${session}",
          "implementation": "org.apache.maven.execution.MavenSession"
        },
        "anotherThingy": {
          "default-value": "foo",
          "implementation": "java.lang.String"
        },
        "project": {
          "default-value": "${project}",
          "implementation": "org.apache.maven.project.MavenProject"
        },
        "skip": {
          "default-value": false,
          "implementation": "boolean",
          "content": "${helper.skip}"
        }
      },
      "requiresProject": true,
      "aggregator": false,
      "implementation": "org.sonatype.dionysus.example.maven.HelperMojo",
      "description": "Helper mojo.",
      "threadSafe": false,
      "requiresReports": false,
      "language": "java",
      "requiresDirectInvocation": false,
      "requiresOnline": false,
      "instantiationStrategy": "per-lookup",
      "executionStrategy": "once-per-session",
      "parameters": {"parameter": [
        {
          "editable": true,
          "name": "anotherThingy",
          "description": "Another thingy.",
          "type": "java.lang.String",
          "required": true,
          "since": "???"
        },
        {
          "editable": false,
          "name": "project",
          "description": "",
          "type": "org.apache.maven.project.MavenProject",
          "required": false
        },
        {
          "editable": false,
          "name": "session",
          "description": "",
          "type": "org.apache.maven.execution.MavenSession",
          "required": false
        },
        {
          "editable": false,
          "name": "settings",
          "description": "",
          "type": "org.apache.maven.settings.Settings",
          "required": false
        },
        {
          "editable": true,
          "name": "skip",
          "description": "Skip execution.",
          "type": "boolean",
          "required": false,
          "since": "???"
        }
      ]},
      "since": "???"
    }
  ]},
